<h1>Listing device_models</h1>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Description</th>
      <th>Numberofport</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @device_models.each do |device_model| %>
      <tr>
        <td><%= device_model.model %></td>
        <td><%= device_model.description %></td>
        <td><%= device_model.numberOfPort %></td>
        <td><%= link_to 'Show', device_model %></td>
        <td><%= link_to 'Edit', edit_device_model_path(device_model) %></td>
        <td><%= link_to 'Destroy', device_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

